          <div class="form-group">
            <label>Producto</label>
            <input id="nombre" class="form-control" type="text" name="nombre" />
          </div>
          <div class="form-group">
            <label>imagen</label>
            <input id="imagen" class="form-control" type="text" name="imagen" />
          </div>
          <div class="form-group">
            <label>Precio</label>
            <input
              id="precio"
              class="form-control"
              type="number"
              name="precio"
            />
          </div>
          <button class="btn btn-success mt-3 mb-5">Enviar</button>
        </form>
        <h2 style="color: rgb(77, 68, 68)">Historial</h2>
        <%- include('./tabla')-%>
      </div>
    </div>
  </body>
</html>
